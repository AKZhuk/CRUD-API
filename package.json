{
  "name": "simple-crud-api",
  "version": "1.0.0",
  "description": "My webpack project",
  "main": "index.ts",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start:dev": "ts-node-dev  --env-file=.env --respawn src/index.ts",
    "start:prod": "npm run build && node --env-file=.env dist/bundle.js ",
    "start:multi": "cross-env SERVER_MODE=MULTI ts-node-dev --env-file=.env --respawn src/index.ts",
    "test": "jest",
    "build": "webpack --mode=production --node-env=production",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\""
  },
  "author": "Aliaksei Zhuk",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.3.1",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "@webpack-cli/generators": "^3.0.7",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.1",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "nodemon": "^3.0.3",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "uuid": "^9.0.1"
  }
}
